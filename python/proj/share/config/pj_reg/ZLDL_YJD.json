            },          
            "[23]": {
                "RW": "RW",
                "definition": "INVERT",
                "description": "阈值比较翻转：为1时，同一周期事件数小于阈值时计数器加一.",
                "reset_value": "1'b0",
                "ucode_reset_value": "1'b0"
            },          
            "[31:24]": {
